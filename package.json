    "@umijs/test": "^3.2.25",
    "echarts": "4.9.0",
    "echarts-for-react": "^2.0.16",
    "lint-staged": "^10.0.7",
    "lodash-es": "^4.17.15",
    "open-browser-webpack-plugin": "0.0.5",
    "prettier": "^1.19.1",
    "qs": "^6.9.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.25",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/echarts": "4.9.0"
  }
}
